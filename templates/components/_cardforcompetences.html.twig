<div class="row">
  {% for competence in competences %}
  <div class="cards-list col">
  <div class="card_1">
    <div class="card_image"> <img src="https://i.redd.it/b3esnz5ra34y.jpg" /> </div>
    <div class="card_title title-white">
      <p class="card-text fw-bold text-intermediate text-center fs-5 m-auto">{{competence.nom}}</p>
      <p class="card-text fw-bold text-intermediate m-auto text-center">{{competence.niveau}}</p>
      <p class="card-text fw-bold text-intermediate text-center">{{competence.experience.id}}</p>
      <p class="card-text fw-bold text-intermediate text-center">{{competence.formation.id}}</p>
    </div>
    </div>
  </div>
  {% endfor %}
</div>